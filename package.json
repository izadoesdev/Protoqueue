{
  "name": "protoqueue",
  "version": "0.1.0",
  "description": "A lightweight, high-performance queue system built on NATS and Protocol Buffers",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist",
    "dev": "bun --watch ./src/index.ts",
    "example": "bun run ./src/example.ts",
    "test": "bun test",
    "lint": "tsc --noEmit",
    "clean": "rm -rf ./dist",
    "prepublish": "bun run clean && bun run build"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.15.12",
    "google-protobuf": "^3.21.4",
    "nanoid": "^5.1.5",
    "nats": "^2.16.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "protobufjs": "^7.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/node": "^22.14.1",
    "bun-types": "latest",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}